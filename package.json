{
	"name": "bigbox",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "pnpm run build:sandbox && pnpm run build:pwa",
		"build:sandbox": "pnpm --filter @bigbox/sandbox-manager build",
		"build:pwa": "pnpm --filter bigbox-pwa build",
		"dev": "pnpm --filter bigbox-pwa dev",
		"preview": "pnpm --filter bigbox-pwa preview",
		"install:all": "pnpm install"
	},
	"dependencies": {
		"react-icons": "^5.5.0"
	},
	"devDependencies": {
		"workbox-window": "^7.3.0"
	}
}
